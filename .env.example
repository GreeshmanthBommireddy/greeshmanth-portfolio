# Copy this file to `.env` and fill in values for local development and set the same keys in Vercel Project Settings

# MongoDB connection string (required for contact form / DB features)
MONGODB_URI=mongodb+srv://<username>:<password>@cluster0.mongodb.net/<dbname>?retryWrites=true&w=majority

# Twilio (optional - for WhatsApp notifications)
TWILIO_ACCOUNT_SID=ACxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
TWILIO_AUTH_TOKEN=your_auth_token
TWILIO_WHATSAPP_NUMBER=whatsapp:+1234567890

# Node env (optional)
NODE_ENV=production
